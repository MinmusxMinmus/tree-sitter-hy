{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hy",
  "word": "symbol",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "shebang"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_item"
          }
        }
      ]
    },
    "shebang": {
      "type": "PATTERN",
      "value": "#!~?(?:\\/(\\w*))+(?: .*)?"
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "_form": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_literal"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "_keywords_with_values"
        },
        {
          "type": "SYMBOL",
          "name": "_string"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        },
        {
          "type": "SYMBOL",
          "name": "set"
        },
        {
          "type": "SYMBOL",
          "name": "dictionary"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_expression"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\("
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "PATTERN",
          "value": "\\)"
        }
      ]
    },
    "quoted_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": ";.*"
    },
    "discard_prefix": {
      "type": "PATTERN",
      "value": "#_"
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "PATTERN",
          "value": "]"
        }
      ]
    },
    "set": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "#\\{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_form"
          }
        },
        {
          "type": "PATTERN",
          "value": "}"
        }
      ]
    },
    "dictionary": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "dictionary_item"
          }
        },
        {
          "type": "PATTERN",
          "value": "}"
        }
      ]
    },
    "dictionary_item": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_form"
        },
        {
          "type": "SYMBOL",
          "name": "_form"
        }
      ]
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[^\\s\\(\\)\\[\\]\\{\\}#;:\"'`~][^\\s\\(\\)\\[\\]\\{\\}#;\"'`~]*"
    },
    "keyword": {
      "type": "PATTERN",
      "value": "[^\\s\\(\\)\\[\\]\\{\\}#;\"'`~]+"
    },
    "_keywords_with_values": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "none"
        }
      ]
    },
    "_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_number"
        }
      ]
    },
    "_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "integer"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "imaginary_number"
        }
      ]
    },
    "integer": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[+-]?"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[1-9](?:_?[0-9])*"
              },
              {
                "type": "PATTERN",
                "value": "0[bB](?:_?[01])+"
              },
              {
                "type": "PATTERN",
                "value": "0[oO](?:_?[0-7])+"
              },
              {
                "type": "PATTERN",
                "value": "0[xX](?:_?[a-fA-F0-9])+"
              }
            ]
          }
        ]
      }
    },
    "float": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[+-]?"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "(?:(?:[0-9](?:_?[0-9])*)?\\.(?:[0-9](?:_?[0-9])*))|((?:[0-9](?:_?[0-9])*)\\.)"
              },
              {
                "type": "PATTERN",
                "value": "(?:(?:[0-9](?:_?[0-9])*)|(?:(?:[0-9](?:_?[0-9])*)?\\.(?:[0-9](?:_?[0-9])*))|((?:[0-9](?:_?[0-9])*)\\.))[eE][-+]?(?:[0-9](?:_?[0-9])*)"
              }
            ]
          }
        ]
      }
    },
    "imaginary_number": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[+-]?"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "PATTERN",
            "value": "(?:(?:(?:(?:[0-9](?:_?[0-9])*)?\\.(?:[0-9](?:_?[0-9])*))|((?:[0-9](?:_?[0-9])*)\\.)|(?:(?:[0-9](?:_?[0-9])*)|(?:(?:[0-9](?:_?[0-9])*)?\\.(?:[0-9](?:_?[0-9])*))|((?:[0-9](?:_?[0-9])*)\\.))[eE][-+]?(?:[0-9](?:_?[0-9])*))|(?:[0-9](?:_?[0-9])*))[jJ]"
          }
        ]
      }
    },
    "boolean": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "True"
          },
          {
            "type": "STRING",
            "value": "False"
          }
        ]
      }
    },
    "none": {
      "type": "STRING",
      "value": "None"
    },
    "_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "fstring"
        }
      ]
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "(?:\\\\.)|[^\"]"
          }
        },
        {
          "type": "PATTERN",
          "value": "\""
        }
      ]
    },
    "fstring": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "f\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": "(?:\\\\.)|[^\"\\{]"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "PATTERN",
                        "value": "\\{"
                      },
                      {
                        "type": "SYMBOL",
                        "name": "_form"
                      },
                      {
                        "type": "PATTERN",
                        "value": "\\}"
                      }
                    ]
                  },
                  {
                    "type": "BLANK"
                  }
